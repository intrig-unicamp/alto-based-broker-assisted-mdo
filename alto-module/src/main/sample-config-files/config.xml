<?xml version="1.0" encoding="UTF-8"?>
<config>        
    <AltoPlugin>
	<AltoDB>
	    <serverIP>bolt://127.0.0.1</serverIP>
            <serverPort>7687</serverPort>

	    <networkMap>	        
	        <name>my-default-network-map</name>		
	    </networkMap>
        </AltoDB>

	<AggregatorDB>
	    <serverIP>bolt://127.0.0.1</serverIP>
            <serverPort>7687</serverPort>
        </AggregatorDB>	
    </AltoPlugin>

    <ALTO>
	<server>
            <serverPort>8181</serverPort>
        </server>

        <AltoDB>
	    <serverIP>bolt://127.0.0.1</serverIP>
            <serverPort>7687</serverPort>
        </AltoDB>
    </ALTO>

    <UNIFY>
        <Export> 
            <serverPort>8090</serverPort>
        </Export>

        <AltoServer>
	    <serverIP>http://127.0.0.1</serverIP>
            <serverPort>8181</serverPort>
        </AltoServer>

        <networkMap>
	    <url>/networkmap</url> 	
            <contentType>application/alto-networkmap+json</contentType> 	
	    <name>my-default-network-map</name>		
	</networkMap>
    </UNIFY>       
</config>
